                        <li><a href="https://powerhousea01zerowaste.azurewebsites.net/Home/Contact"><i class="flaticon-food-1"></i>Contact us</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </header>

    <section class="inner-banner">
        <div class="thm-container">
            <div class="box clearfix">
                <h2 class="pull-left">Recipes</h2>
                <ul class="breadcumb pull-right">
                    <li><span>You can see the recipe you search here</span></li>
                    </ul>
            </div>
        </div>
    </section>


    <section class="single-recipe-page-content sec-padding">
        <div class="thm-container">
            <div class="row">
                <div class="col-md-7">
                    <div class="">
                        <div id="imgs">
                            <!--recipe images/ -->

                        </div>

                    </div>


                    <div class="sec-title">

                        <!--Step/-->

                        <h2>Steps</h2>
                    </div>

                    <div class="direction-box" id="steps">


                    </div>

                </div>
                <div class="col-md-5">
                    <div class="content-box-wrapper">
                        <div class="title-box">
                            <div class="top-meta clearfix">

                            </div>
                            <!-- Title -->
                            <div class="title" id="name">

                            </div>
                        </div>

                        <div class="ingredients contact-form">
                            <div class="sec-title">
                                <h2>ingredients</h2>
                            </div>
                            <ul class="special-checkbox" id="ingredient">
                            </ul>
                        </div>
                        <div class="sec-title">
                            <!--Summary/-->
                            <h2>Descriptions</h2>
                        </div>
                        <div id="summary">

                        </div>
                        <div class="sec-title">

                            <!--Nurtrition/-->

                            <h2>nutrition</h2>
                        </div>
                        <div class="nutrition-box">
                            <ul>
                                <li>
                                    <!--Protein/-->
                                    <span class="name">Protein</span>
                                    <span class="weight" id="protien"></span>

                                </li>

                                <li>
                                    <!--Fat/-->
                                    <span class="name">Fat</span>
                                    <span class="weight" id="fat"></span>

                                </li>
                                <!--Carbohydrates/-->

                                <li>
                                    <span class="name">Carbohydrates</span>
                                    <span class="weight" id="carbohydrate"></span>

                                </li>
                                <!--Calories/-->

                                <li>
                                    <span class="name">Calories</span>
                                    <span class="weight" id="calories"></span>

                                </li>

                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>




